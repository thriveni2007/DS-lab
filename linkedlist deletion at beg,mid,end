#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *link;
};
struct node *dd_end(struct node *head){
    struct node *ptr=malloc(sizeof(struct node));
    struct node *temp=malloc(sizeof(struct node));
    ptr=head;
    temp=head;
    while(ptr->link!=NULL){
        temp=ptr;
        ptr=ptr->link;
    }
    temp->link=NULL;
    return head;
}

struct node *dd_beg(struct node *head){
    head=head->link;
    return head;
    
}
struct node *dd_midd(struct node *head){
    struct node *temp=malloc(sizeof(struct node));
    struct node *ptr=malloc(sizeof(struct node));
    temp=head;
    ptr=head;
    int pos=3;
    int i=1;
    while(i<pos){
       ptr=temp;
       temp=temp->link;
       i=i+1;
    }
    ptr->link=temp->link;
    return head;
}

struct node *display(struct node *head){
    struct node *temp=head;
    while(temp!=NULL){
        printf("%d",temp->data);
        temp=temp->link;
    }
    
}
int main(){
    struct node *head=malloc(sizeof(struct node));
    struct node *head1=malloc(sizeof(struct node));
    struct node *head2=malloc(sizeof(struct node));
    struct node *head3=malloc(sizeof(struct node));
    struct node *head4=malloc(sizeof(struct node));
    head->data=10;
    head1->data=20;
    head2->data=30;
    head3->data=40;
    head4->data=50;
    head->link=head1;
    head1->link=head2;
    head2->link=head3;
    head3->link=head4;
    head4->link=NULL;
    struct node *a;
    struct node *b;
    struct node *c;
    struct node *d;
    d=display(head);
    printf("\n");
    a=dd_end(head);
    d=display(head);
    printf("\n");
    head=dd_beg(head);
    d=display(head);
    printf("\n");
    c=dd_midd(head);
    d=display(head);
}
