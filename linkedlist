#include <stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *link;
};
 struct node* push(int num,struct node *head){
    struct node *ptr=malloc(sizeof(struct node));
    struct node *temp=head;
    ptr->data=num;
    ptr->link=NULL;
    if(head==NULL)
    return ptr;
    else
    {
        while(temp->link!=NULL){
        temp=temp->link;
    }
    temp->link=ptr;
    return head;}
}
void display(struct node *head){
     struct node *ptr=head;
    while(ptr!=NULL){
        printf("%d",ptr->data);
        ptr=ptr->link;
    }
}
int main() {
     struct node *head=NULL;
   head=push(10,head);
   head=push(20,head);
   head=push(30,head);
   display(head);
}
