#include <stdio.h>
#include<stdlib.h>
#define max 10
int arr[max];
int front=-1;
int rear=-1;
int enqueue(int value){
   if(front==-1){
       front++;
       arr[front]=value;
   } 
   if(rear==max-1){
       printf("queue is full \n");
   }
   else{
       rear++;
   arr[rear]=value;}
}
void dequeue(){
    int num;
    if(front==rear+1){
        printf("no element found\n");
    }
    else{
        num=arr[front];
        front++;
        printf("%d is deleted \n",num);
    }
}
void display(){
    for(int i=front;i<rear;i++){
        printf("%d",arr[i]);
    }
}
int main() {
    int value;
     int a;
     do{
         printf("enter 1:insert \n enter 2: deletion \n enter 3:no of customer in queue \n enter 4:exit");
        printf("\n enter your choice:");
        scanf("%d",&a);
        switch(a){
            case 1:printf("enter value");
            scanf("%d",&value);
            enqueue(value);
            break;
            case 2:dequeue();
            break;
            case 3:display();
            break;
            case 4:exit(0);
            break;
            
        }
     }while(a!=4);

    return 0;
}
